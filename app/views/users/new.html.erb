<div class="row">
  <div class="col-md-12">
    <h1 class="text-center">Sign Up</h1>
      <%= form_for @user do |f|%>
        <%= f.label :username %><br>
        <%= f.text_field :username, class: "form-control" %><br>
        <%= f.label :password %><br>
        <%= f.password_field :password, class: "form-control" %><br>
        <%= f.submit class: "btn btn-primary" %>
      <%end%>
  </div>
</div>

